{
  "name": "jsx-map",
  "version": "0.0.2",
  "description": ".map() for JSX",
  "main": "index.build.js",
  "scripts": {
    "test": "ava index.test.js",
    "lint": "eslint index.js index.test.js",
    "build": "babel index.js --out-file index.build.js",
    "prepublish": "npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vahnag/jsx-map.git"
  },
  "keywords": [
    "jsx",
    "map",
    "react"
  ],
  "author": "Vahagn Aharonian <vahnag@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vahnag/jsx-map/issues"
  },
  "homepage": "https://github.com/vahnag/jsx-map#readme",
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.8.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.0",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.0.1",
    "react": "^15.0.2",
    "react-addons-test-utils": "^15.0.2",
    "react-dom": "^15.0.2"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      [
        "transform-es2015-modules-umd",
        {
          "globals": {
            "react": "React"
          }
        }
      ]
    ],
    "moduleIds": true,
    "moduleId": "jsxMap"
  },
  "dependencies": {
    "eslint": "^2.9.0"
  }
}
